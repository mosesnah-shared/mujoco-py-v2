        <mesh name="panda_link_6_vis" file="meshes/link6_vis.stl" />
        <mesh name="panda_link_7_vis" file="meshes/link7_vis.stl" />
        <mesh file="meshes/pedestal.stl" name="panda_pedestal"/>
